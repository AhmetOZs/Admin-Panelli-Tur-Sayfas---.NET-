@using WebApplication1.Models.sınıflar;
@model List<TurTarihRU>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/Adminlayout.cshtml";
}

<table class="table table-bordered" style="margin-top:20px">
    <tr>
        <th>Tarihler </th>
        
        

    </tr>
    @foreach (var k in Model)
    {

        <tr>
            
           
            
            <td> @k.tarih</td>
           
            
            
         <td><a href="@Url.Action("TarihSil","TurTarihRU",new {id=k.tarihID})" class="btn btn-danger">Sil</a></td>
        <td><a href="@Url.Action("TarihGetir","TurTarihRU",new {id=k.tarihID})" class="btn btn-success">Güncelle</a></td>
          </tr>
    }
</table>
<a href="/TurTarihRU/TarihEkle" class="btn btn-info">Tarih Ekle</a>